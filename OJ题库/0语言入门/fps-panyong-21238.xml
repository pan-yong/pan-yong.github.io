		<solution language="C"><![CDATA[#include<stdio.h>

int main()
{
	int a,b,c,d,e,f;
	scanf("%d%d",&a,&b);
	c=a+b;
	d=a-b;
	e=a*b;
	f=a/b;
	printf("%d+%d=%d\n%d-%d=%d\n%d*%d=%d\n%d/%d=%d\n",a,b,c,a,b,d,a,b,e,a,b,f);
	return 0;
}]]></solution>
			<solution language="C++"><![CDATA[#include<stdio.h>

int main()
{
	int a,b,c;
	scanf("%d%d",&a,&b);
	c=a+b;
	printf("%d+%d=%d\n",a,b,c);
	c=a-b;
	printf("%d-%d=%d\n",a,b,c);
	c=a*b;
	printf("%d*%d=%d\n",a,b,c);
	c=a/b;
	printf("%d/%d=%d\n",a,b,c);
	return 0;
}]]></solution>
			<solution language="Pascal"><![CDATA[var a,b:integer;
begin
  readln(a,b);
  writeln(a,'+',b,'=',a+b);
  writeln(a,'-',b,'=',a-b);
  writeln(a,'*',b,'=',a*b);
  if a mod b =0 then
  writeln(a,'/',b,'=',a/b:0:0);
end.]]></solution>
